{
  "expo": {
    "name": "quasar",
    "slug": "montyhacks-viii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/appicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shashankprasanna.quasar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shashankprasanna.quasar"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "4e8217c4-3790-4b43-9711-278e5f9f29f6"
      },
      "androidNavigationBar":{
        "visible": "sticky-immersive"
      }
    }
  }
}
